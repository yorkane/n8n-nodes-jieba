{
  "name": "n8n-nodes-klib",
  "version": "1.0.0",
  "description": "n8n node build from https://github.com/napi-rs/node-rs",
  "keywords": [
    "n8n-community-node-package",
    "jieba",
    "chinese",
    "segmentation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yorkane/n8n-nodes-jieba",
  "author": {
    "name": "Yorkane",
    "email": "yorkane@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yorkane/n8n-nodes-klib.jieba"
  },
  "scripts": {
    "build": "npx tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist",
    "lib"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/ImageProcessor/ImageProcessor.node.js",
      "dist/nodes/Aria2/Aria2.node.js",
      "dist/nodes/Aria2/Aria2Proc.node.js",
      "dist/nodes/TorrentParse/TorrentParse.node.js",
      "dist/nodes/FsUtils/FsUtils.node.js",
      "dist/nodes/FsOperate/FsOperate.node.js",
      "dist/nodes/Jieba/JiebaSegment.node.js"
    ]
  },
  "dependencies": {
    "@node-rs/jieba": "^2.0.1",
    "pnpm": "^10.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^4.1.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
